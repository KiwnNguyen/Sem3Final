
@{
    ViewBag.Title = "FormEditImage";
}

<section id="aa-contact">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="aa-contact-area">
                    <div class="aa-contact-top">
                        <h2>Thay đổi giao diện</h2>
                    </div>
                    <!-- contact map -->
                    <!-- Contact address -->
                    <div class="aa-contact-address">
                        <div class="row">
                            <div class="col-md-8">
                                <div class="aa-contact-address-left">
                                    <form class="comments-form contact-form" action="/Home/UpdateImageUser" method="post" enctype="multipart/form-data">
                                        <div class="form-group">
                                            <input type="file" name="image" value="" />
                                        </div>
                                        <div class="form-group">

                                            <input type="text" name="id" value="@HttpContext.Current.Session["IdAccountUser"]" style=" display: none" />
                                        </div>
                                        <button class="aa-secondary-btn">Send</button>
                                    </form>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>


